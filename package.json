{
  "name": "thai-alphabet-sort",
  "version": "0.0.15",
  "description": "Comparator for thai word base on thai alpahbet order",
  "keywords": [
    "thai",
    "alphabet",
    "sort"
  ],
  "main": "lib/tstrcmp.js",
  "types": "lib/tstrcmp.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "deploy": "gh-pages -d dist",
    "build-npm": "tsc"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "eslint": "^7.27.0",
    "umi": "^3.4.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.20",
    "gh-pages": "^3.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matrixyf/thai_alphabet_sort.git"
  },
  "bugs": {
    "url": "https://github.com/matrixyf/thai_alphabet_sort/issues"
  },
  "homepage": "https://github.com/matrixyf/thai_alphabet_sort/blob/main/README.md"
}
